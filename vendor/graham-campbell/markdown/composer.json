{
    "name": "graham-campbell/markdown",
    "description": "Markdown Is A CommonMark Wrapper For Laravel",
    "keywords": ["laravel", "framework", "markdown", "commonmark", "common mark", "Laravel Markdown", "Laravel-Markdown", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "illuminate/view": "^5.5|^6.0|^7.0",
        "league/commonmark": "^1.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4",
        "graham-campbell/testbench": "^5.4",
        "league/commonmark-extras": "^1.0",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Markdown\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "11.2-dev"
        },
        "laravel": {
            "providers": [
                "GrahamCampbell\\Markdown\\MarkdownServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
